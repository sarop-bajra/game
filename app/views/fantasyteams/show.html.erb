<h2>
  <%= @fantasyteam.name %>
</h2>
<% if @fantasyteam.user_id == @current_user.id %>
  <%= link_to 'Edit Your Fantasy Team', edit_fantasyteam_path(@fantasyteam) %>
<% end %>

# player link to come and team link to come
<ul style="list-style-type:none;">
  <% @fantasyteam.players.each do |p| %>
  <div class="fantasyteam">
  <li>
    <%= link_to image_tag p.image %>
    <%# <!-- <%= link_to image_tag p.team.image %>
  </li>
  </div>
  <% end %>
</ul>
